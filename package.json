{
  "name": "nice-node",
  "version": "1.2.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "nice-node": "dist/bin/nice-node.js"
  },
  "files": [
    "dist",
    "scripts",
    ".syncignore",
    "nicenode.env",
    "tsconfig.json"
  ],
  "repository": "nice-node/nice-node",
  "scripts": {
    "test": "PROFILE=local NODE_ENV=test mocha",
    "test2": "ts-node -r tsconfig-paths/register test/middlewares/test.ts",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepublish": "npm run build",
    "start": "nodemon",
    "lint": "eslint \"src/**/*.{ts,js,tsx,jsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "standard-version && npm publish",
    "release:major": "standard-version --release-as major && npm publish",
    "release:minor": "standard-version --release-as minor && npm publish",
    "release:patch": "standard-version --release-as patch && npm publish",
    "release:alpha": "standard-version --prerelease alpha && npm publish --tag=next",
    "dist-tag": "npm dist-tag add nice-node@1.6.0 latest && npm dist-tag add nice-node@2.0.0-alpha.4 next",
    "release:major:alpha": "standard-version --release-as major --prerelease alpha && npm publish"
  },
  "engines": {
    "node": ">= 8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,tsx,jsx}": "eslint"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/joi": "^14.3.4",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "^7.4.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.4",
    "@types/qs": "^6.9.1",
    "@types/rimraf": "^2.0.4",
    "@types/statsd-client": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "axios": "^0.21.1",
    "cli-table-fix": "^0.3.1",
    "date-fns": "^2.11.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-qunar-typescript-node": "^1.1.16",
    "file-ignore-parser": "^2.0.0",
    "file-stream-rotator": "^0.5.7",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "joi": "^14.3.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lint-staged": "10.0.8",
    "nodemon": "^2.0.2",
    "npmlog": "^4.1.2",
    "qs": "^6.9.3",
    "require-all": "^3.0.0",
    "rimraf": "^3.0.2",
    "statsd-client": "^0.4.4",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@qnpm/QRedis": "^1.0.6",
    "@qnpm/mysql-pxc": "^0.2.0",
    "@qnpm/qconfig-client-plus": "0.0.14",
    "@types/graphql-type-json": "^0.3.2",
    "@types/koa-graphql": "^0.8.3",
    "@types/mocha": "^7.0.2",
    "@types/npmlog": "^4.1.2",
    "@types/should": "^13.0.0",
    "@types/supertest": "^2.0.8",
    "coveralls": "^3.0.11",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "graphql-type-json": "^0.3.1",
    "knex": "^0.20.13",
    "koa-graphql-fix": "^0.8.1",
    "koa-pug": "^4.0.3",
    "merge-graphql-schemas": "^1.7.6",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "report-dir": "./coverage"
  }
}
